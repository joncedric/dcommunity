<%= form_tag(dance_classes_path, method: "get")  do %>
  <%= select_tag "query", options_from_collection_for_select(@dance_styles, "id", "name") %>
  <%= submit_tag("Search") %>
<% end %>

<div class="row">
  <div class="small-12 large-9 columns large-centered">
    <h2>Upcoming Events</h2>
    <%= cell(:event, collection: Event.all) %>
  </div>
</div>
